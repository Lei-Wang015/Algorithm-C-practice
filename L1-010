#include <stdio.h>

int main(){
    int nums[3];
    for(int i = 0;i < 3;i++){
        scanf("%d", &nums[i]);
    }
    //少一轮循环
    for(int j = 0;j < 3;j++){
        for(int i = 0;i < 2;i++){
            if(nums[i] > nums[i + 1]){
                int temp = nums[i];
                nums[i] = nums[i + 1];
                nums[i + 1] = temp;
        }
    }
    }
    for(int i = 0;i < 2;i++){
        printf("%d->",nums[i]);
    }
    printf("%d\n",nums[2]);
    return 0;
}
